{
  "name": "test",
  "version": "1.0.0",
  "main": "src/js/app.js",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "clean": "astro clean",
    "lint:es": "eslint --cache 'src/**/*.{astro,js,ts,json}'",
    "lint:style": "stylelint --fix \"src/**/*.scss\"",
    "lint:html": "markuplint \"src/**/*.astro\"",
    "format": "prettier ./src --write --ignore-path .gitignore",
    "commitmsg": "commitlint --edit ${1}",
    "prepare": "husky install",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "validate": "npm run check-types && lint-staged --no-stash",
    "new:ac": "hygen new ac"
  },
  "lint-staged": {
    "*.astro": [
      "markuplint --config .markuplintrc.js"
    ],
    "*.{astro,js,ts}": [
      "eslint --fix --cache"
    ],
    "*.scss": [
      "stylelint --fix"
    ],
    "*.{astro,scss,js,ts,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@kokorotobita/paginize": "^1.0.4",
    "astro": "^2.0.11",
    "library-test-kokorotobita": "^1.0.0"
  },
  "devDependencies": {
    "@astrojs/image": "^0.15.0",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@markuplint/astro-parser": "^3.6.3",
    "@rollup/plugin-yaml": "^4.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "astro-compress": "1.1.42",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-astro": "^0.21.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "8.0.2",
    "hygen": "^6.2.11",
    "lint-staged": "13.0.3",
    "markuplint": "^3.9.1",
    "prettier": "^2.8.1",
    "prettier-plugin-astro": "^0.7.2",
    "sass": "^1.57.1",
    "stylelint": "14.16.1",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-scss": "4.4.0"
  }
}
